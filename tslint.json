{
  "rules": {
    "extends": "tslint-config-airbnb",
    "max-line-length": {
      "options": [
        120
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "prefer-const": true,
    "eofline": true,
    "semicolon": true,
    // "quotemark": [
    //   true,
    //   "single",
    //   "double"
    // ],
    "no-console": {
      "options": [
        // "log",
        "debug",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}
